{
  "name": "rtterror-backend",
  "version": "1.0.0",
  "description": "nodejs+expressjs backend",
  "main": "./src/server.ts",
  "scripts": {
    "test": "jest --watchAll",
    "coverage": "jest --coverage",
    "start": "ts-node-dev --respawn --transpile-only ./src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "rtterror"
  },
  "author": "rtterror",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mysql2": "^2.3.0",
    "sequelize": "^6.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/validator": "^13.7.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6",
    "ts-node-dev": "^1.1.8"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "./**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 80
      }
    }
  }
}
